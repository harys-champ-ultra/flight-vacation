    <main>
        <div class="grids">
            <h1><%= heading %></h1>
            <% data.forEach(eachData => { %>
            <section>
                <figure>
                    <img src="img/<%= eachData.img %>" alt="Packages">
                </figure>
                <h2><%= eachData.title %></h2>
                <p>$<%= eachData.price %></p>
                <ul>
                    <li>
                        <form action="/followPackages" method="post">
                            <% dataUser.forEach(onlyData => { %>
                            <input type="number" name="userid" id="userid" value="<%= onlyData.id %>" hidden>
                            <% }); %> 
                            <button type="submit" value="<%= eachData.id %>" name="packageNumber"><%= buttonFollow %></button>
                        </form>
                    </li>
                </ul>
                <ul>
                    <li>
                        <form action="/addCart" method="post">
                            <% dataUser.forEach(onlyData => { %>
                            <input type="number" name="userid" id="userid" value="<%= onlyData.id %>" hidden>
                            <% }); %>
                            <input type="number" name="packageQuant" id="packageQuant" min="1" max="20" placeholder="Quantity" required>
                            <button type="submit" value="<%= eachData.id %>" name="packageAdd"><%= buttonCart %></button>
                        </form>
                    </li>
                </ul>
            </section>
            <% }); %> 
        </div>
    </main>
